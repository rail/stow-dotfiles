alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north

shift + alt - h : yabai -m window --swap west --focus
shift + alt - l : yabai -m window --swap east --focus
shift + alt - j : yabai -m window --swap south --focus
shift + alt - k : yabai -m window --swap north --focus

alt - 0 : yabai -m space --balance
alt - 1 : yabai -m display --focus 1
alt - 2 : yabai -m display --focus 2
alt - r : yabai -m space --rotate 270

cmd + ctrl - 1 : yabai -m window --space 1
cmd + ctrl - 2 : yabai -m window --space 2
cmd + ctrl - 3 : yabai -m window --space 3
cmd + ctrl - 4 : yabai -m window --space 4
cmd + ctrl - 5 : yabai -m window --space 5
cmd + ctrl - 6 : yabai -m window --space 6
cmd + ctrl - 7 : yabai -m window --space 7
cmd + ctrl - 8 : yabai -m window --space 8
cmd + ctrl - 9 : yabai -m window --space 9

alt - x : yabai -m space --mirror x-axis
alt - y : yabai -m space --mirror y-axis
alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

cmd + ctrl - r : yabai --stop-service; yabai --start-service
